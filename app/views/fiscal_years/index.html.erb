<div class="row">
  <table class="table table-striped table-bordered">
    <div class="action text-right">
      <a href="<%= new_fiscal_year_path %>" class="btn btn-primary"><span><i class="fa fa-plus-square"></i></span>New</a>

    </div>
    <tr>
      <th>Name</th>
      <th>Start-date</th>
      <th>End-date</th>
      <th>Actions</th>
    </tr>
    <% @fiscal_years.each do |fiscal_years| %>
        <tr>
          <td><%= fiscal_years.name %></td>
          <td><%= fiscal_years.start_date %></td>
          <td><%= fiscal_years.end_date %></td>
          <td><%= link_to "", edit_fiscal_year_path(fiscal_years), :class => "fa fa-pencil", :title => "Edit" %>
          <%= link_to "", fiscal_year_path(fiscal_years), :class => "fa fa-trash", :title => "delete",
                          method: :delete,
                          data: {confirm: "Are you sure"} %></td>
        </tr>
    <% end %>
  </table>
</div>