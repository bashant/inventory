<div class="row">
  <table class="table table-striped table-bordered">
    <tr>
      <th>User-Id</th>
      <th>Description</th>
      <th>Created-At</th>
    </tr>
    <% @logs.each do |logs| %>
        <tr>
          <td><%= logs.user.email%></td>
          <td><%= logs.description %></td>
          <td><%= logs.created_at%></td>
        </tr>

    <% end %>
  </table>
</div>